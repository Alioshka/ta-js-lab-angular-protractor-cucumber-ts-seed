{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "keywords": [
      "Protractor",
      "Cucumber",
      "TypeScript",
      "Angular",
      "Seed"
  ],
  "main": "index.js",
  "scripts": {
      "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
      "pree2e": "npm run webdriver:update",
      "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
      "lite": "lite-server",
      "tsc": "tsc",
      "tsc:w": "tsc -w",
      "webdriver:update": "webdriver-manager update --versions.standalone 3.4.0"
  },
  "author": "M2JS",
  "dependencies": {
      "@angular/common": "5.2.8",
      "@angular/compiler": "5.2.8",
      "@angular/core": "5.2.8",
      "@angular/forms": "5.2.8",
      "@angular/http": "5.2.8",
      "@angular/platform-browser": "5.2.8",
      "@angular/platform-browser-dynamic": "5.2.8",
      "@angular/router": "5.2.8",
      "@angular/upgrade": "5.2.8",
      "angular-in-memory-web-api": "0.5.4",
      "bootstrap": "3.3.7",
      "systemjs": "0.19.39",
      "core-js": "2.4.1",
      "reflect-metadata": "0.1.8",
      "rxjs": "5.5.0",
      "zone.js": "^0.8.4"
  },
  "devDependencies": {
      "chai": "3.5.0",
      "chai-as-promised": "5.3.0",
      "cucumber": "1.2.2",
      "protractor-cucumber-framework": "0.6.0",
      "protractor": "^5.2.0",
      "ts-node": "5.0.1",
      "typescript": "2.6.1",
      "cucumber-tsflow": "1.2.1",
      "concurrently": "3.1.0",
      "lite-server": "2.2.2",
      "canonical-path": "0.0.2",
      "http-server": "0.9.0",
      "lodash": "4.16.2",
      "webdriver-manager": "12.0.6",
      "rimraf": "2.5.4",
      "gulp": "3.9.1",
      "awesome-report-generator": "https://github.com/Alioshka/ta-js-lab-awesome-report-generator.git",
      "@types/core-js": "0.9.34",
      "@types/cucumber": "0.0.32",
      "@types/node": "^6.0.45",
      "@types/selenium-webdriver": "^2.53.32",
      "@types/chai": "^3.4.31"
  },
  "engines": {
      "node": ">=6.9.1",
      "npm": ">=3.10.8"
  },
  "repository": {}
}
